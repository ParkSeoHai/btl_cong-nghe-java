    <script>
    document.addEventListener("DOMContentLoaded", function() {
        const reactionsTotal = document.querySelector(".reactions_total");
        const reactionsDetail = reactionsTotal.querySelector(".reactions_detail");

        // Hiển thị khi hover vào reactions_total
        reactionsTotal.addEventListener("mouseenter", function() {
            reactionsDetail.style.display = "flex";
        });

        // Ẩn khi không hover vào reactions_total và reactions_detail
        reactionsTotal.addEventListener("mouseleave", function(event) {
            // Delay hiding the container to ensure we can check if the mouse entered the container
            setTimeout(function() {
                if (!isMouseOverElement(reactionsTotal) && !isMouseOverElement(reactionsDetail)) {
                    reactionsDetail.style.display = "none";
                }
            }, 200);
        });

        reactionsDetail.addEventListener("mouseleave", function() {
            reactionsDetail.style.display = "none";
        });

        reactionsDetail.addEventListener("mouseenter", function() {
            reactionsDetail.style.display = "flex";
        });

        // Hàm kiểm tra xem con trỏ chuột có hover vào phần tử hay không
        function isMouseOverElement(element) {
            const rect = element.getBoundingClientRect();
            return (
                event.clientX >= rect.left &&
                event.clientX <= rect.right &&
                event.clientY >= rect.top &&
                event.clientY <= rect.bottom
            );
        }
    });
    </script>
    
    
